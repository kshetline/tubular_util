{
  "name": "@tubular/util",
  "version": "4.14.0",
  "description": "Miscellaneous utility functions",
  "main": "dist/cjs/index.js",
  "module": "dist/fesm2015/index.js",
  "esm2015": "dist/index.js",
  "fesm2015": "dist/fesm2015/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "exports": {
    "import": "./dist/fesm2015/index.js",
    "require": "./dist/cjs/index.js",
    "script": "./dist/umd/index.js",
    "default": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=10.24.1"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc && rollup -c -m --bundleConfigAsCjs && webpack",
    "prepublishOnly": "npm run build",
    "lint": "eslint 'src/**/*.ts' '**/*.cjs'",
    "test": "nyc --reporter=html mocha --require ts-node/register src/**/*.spec.ts",
    "test-karma": "karma start"
  },
  "keywords": [
    "canvas",
    "clone",
    "font",
    "string",
    "utility"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.10",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "karma": "^6.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "replace": "^1.2.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": "https://github.com/kshetline/tubular_util.git"
}
