{
  "name": "@tubular/util",
  "version": "4.18.1",
  "description": "Miscellaneous utility functions",
  "module": "dist/index.min.mjs",
  "main": "dist/index.min.cjs",
  "browser": "dist/index.min.js",
  "umd:main": "dist/index.min.js",
  "typings": "dist/index",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.min.mjs",
      "require": "./dist/index.min.cjs",
      "browser": "./dist/index.min.js",
      "default": "./dist/index.min.cjs"
    },
    "./dist/cjs/index.cjs": "./dist/index.min.cjs",
    "./dist/cjs/index.js": "./dist/index.min.cjs",
    "./dist/fesm2015/index.js": "./dist/index.min.mjs",
    "./dist/fesm2015/index.mjs": "./dist/index.min.mjs",
    "./dist/umd/index.js": "./dist/index.min.js"
  },
  "engines": {
    "node": ">=10.24.1"
  },
  "scripts": {
    "build": "rimraf dist/ && rollup -c",
    "prepack": "npm run lint && npm run test && npm run build",
    "lint": "eslint --ext .ts 'src/**/*.ts'",
    "test": "karma start --single-run",
    "test-real-chrome": "karma start karma.conf-rc.js"
  },
  "keywords": [
    "canvas",
    "clone",
    "debounce",
    "font",
    "string",
    "throttle",
    "utility"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/chai": "^4.3.16",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-spies": "^1.0.6",
    "@types/mocha": "^8.2.3",
    "@types/node": "^22.15.31",
    "@types/resemblejs": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.2",
    "chai-spies": "^1.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-chai-friendly": "^1.0.0",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.4",
    "mocha": "^10.6.0",
    "replace": "^1.2.2",
    "resemblejs": "^5.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "~5.2.2"
  },
  "overrides": {
    "@rollup/pluginutils": {
      "rollup": "$rollup"
    }
  },
  "repository": "https://github.com/kshetline/tubular_util.git"
}
